		<solution language="C">#include &lt;stdio.h&gt;
main()
{
  int n,a,sum;
  
  while(scanf("%d",&amp;n)&amp;&amp;n)
   {
       sum=0;
         while(n--)
       { 
         scanf("%d",&amp;a);
         sum+=a;
       }
    printf("%d\n",sum);
   }
return 0;
}</solution>
