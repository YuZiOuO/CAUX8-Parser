		<solution language="C">#include&lt;stdio.h&gt;

int main()
{
	int t,a[1000],i;
	while(scanf("%d",&amp;t)!=EOF)
	{
		for(i=0;i&lt;t;i++)
			scanf("%d",&amp;a[i]);
		for(i=0;i&lt;t;i++)
			printf("%c",a[i]);
		puts("");
	}
	return 0;
}</solution>
